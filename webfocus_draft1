 -*-SET &VG_ENCABEZADO = '"SUPERFICIE DE TEMPORAL SEMBRADA Y COSECHADA"';
 -SET &VL_WHERE = 'WHERE IDVARIABLE EQ ' | '''' | 'II_2_2_1_1' | '''' | ' OR IDVARIABLE EQ ' | '''' | 'II_2_2_1_2' | '''';
 JOIN CLEAR *
 JOIN
 MCE_R_D2_AGRIGAN03.MCE_R_D2_AGRIGAN03.IDANIO IN
 MCE_R_D2_AGRIGAN03 TO UNIQUE MCE_VANIO.MCE_VANIO.IDANIO
 IN MCE_VANIO AS J0
 END
 JOIN
 MCE_R_D2_AGRIGAN03.MCE_R_D2_AGRIGAN03.IDENTIDAD IN
 MCE_R_D2_AGRIGAN03 TO UNIQUE MCE_VENTIDAD.MCE_VENTIDAD.IDENTIDAD
 IN MCE_VENTIDAD AS J1
 END
 JOIN
 MCE_R_D2_AGRIGAN03.MCE_R_D2_AGRIGAN03.IDTIPOCULTIVO IN
 MCE_R_D2_AGRIGAN03 TO UNIQUE MCE_VTIPOCULTIVO.MCE_VTIPOCULTIVO.IDTIPOCULTIVO
 IN MCE_VTIPOCULTIVO AS J2
 END
 TABLE FILE MCE_R_D2_AGRIGAN03
 SUM II_2_2_1_1 AS 'Sembrada,(ha)'
 II_2_2_1_2 AS 'Cosechada,(ha)'
 BY IDANIO NOPRINT
 BY NOMBREANIO AS 'Año'
 BY IDENTIDAD NOPRINT
 BY NOMBREENTIDAD AS 'Entidad federativa'
 ACROSS IDTIPOCULTIVO NOPRINT
 ACROSS NOMBRETIPOCULTIVO AS ''
 WHERE (IDMODALIDAD EQ 2)
 -INCLUDE WHERE_ENT
