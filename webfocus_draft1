 TYPE=HEADING, OBJECT=TEXT,     FONT='Open Sans',     COLOR='WHITE',     STYLE=BOLD,$
 TYPE=REPORT,     COLUMN=N2,     SQUEEZE=33.0000,$
 ENDSTYLE
 -IF HTML EQ 'HTML' THEN GOTO ETFHTM3 ELSE ETFNHTM3;
 -ETFHTM3
 ON TABLE HOLD AS 'HTMFTE' FORMAT HTMTABLE
 -GOTO BRING4;
 -BRING4
 END
 -ETNNF1
 -GOTO BRINFT2;
