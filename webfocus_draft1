 ON TABLE SET STYLE *
 TYPE=REPORT,     UNITS=CM,     SQUEEZE=ON,     ORIENTATION=PORTRAIT, PAGECOLOR=RGB(255 255 255),$
 TYPE=REPORT,     GRID=ON,     FONT='Open Sans',     SIZE=9,     COLOR='BLACK',     BACKCOLOR=RGB(255 255 255),     STYLE=NORMAL,$
 -ETBRING
 TYPE=DATA,   FONT='Open Sans',     SIZE=9,     BACKCOLOR=( 'WHITE' RGB(222 222 222) ), JUSTIFY=LEFT,$
 TYPE=TITLE,    FONT='Open Sans',     SIZE=9,     COLOR='WHITE',     BACKCOLOR='GRAY',     STYLE=BOLD,$
 TYPE=HEADING,     OBJECT=TEXT,      FONT='Open Sans',     COLOR='GRAY',     STYLE=BOLD,$
 TYPE=REPORT,     COLUMN=N2,     SQUEEZE=10.00000,$
 TYPE=REPORT,     COLUMN=N3,     SQUEEZE=23.00000,$
 ENDSTYLE
 -NOENTRAPDF2
 -IF HTML EQ 'HTML' THEN GOTO ETFHTM2 ELSE ETFNHTM2;
 -ETFHTM2
 ON TABLE HOLD AS 'HTMNOTA' FORMAT HTML
 -GOTO BRING3;
 -BRING3
 END
 -GOTO BRINFT;
 -BRINFT
 TABLE FILE MCE_R_VAR_FUENTES
 BY DESCRIPCION_FUENTE
 BY ID_FUENTE
 WHERE IDVARIABLE EQ 'II_2_2_1_1' OR IDVARIABLE EQ 'II_2_2_1_2'
 ON TABLE HOLD AS HTFTE01
 END
 -IF HTML EQ 'PDF' THEN GOTO ETFPDF2 ELSE ETFNPDF2;
 -ETFNPDF2
 TABLE FILE HTFTE01
 BY DESCRIPCION_FUENTE AS 'FTE'
 BY ID_FUENTE NOPRINT
 WHERE DESCRIPCION_FUENTE NE MISSING AND DESCRIPCION_FUENTE NE '' AND DESCRIPCION_FUENTE NE ' '
 ON TABLE HOLD AS HTFTE1
 END
 -IF HTML EQ 'EXL2K' THEN GOTO ETFEXC2 ELSE ETFNEXC2;
 -ETFNEXC2
 ON TABLE SET STYLE *
 TYPE=REPORT,     UNITS=CM,     SQUEEZE=ON,     ORIENTATION=PORTRAIT, PAGECOLOR=RGB(255 255 255),$
 TYPE=REPORT,     GRID=ON,     FONT='Open Sans',     SIZE=9,     COLOR='BLACK',     BACKCOLOR=RGB(255 255 255),     STYLE=NORMAL,$
 -ETBRING2
 TYPE=DATA,     FONT='Open Sans',     SIZE=9,     BACKCOLOR=( 'WHITE' RGB(222 222 222) ),$
 TYPE=TITLE,    FONT='Open Sans',     SIZE=9,     COLOR='WHITE',     BACKCOLOR='GRAY',     STYLE=BOLD, $
 TYPE=HEADING, OBJECT=TEXT,     FONT='Open Sans',     COLOR='WHITE',     STYLE=BOLD,$
 TYPE=REPORT,     COLUMN=N2,     SQUEEZE=33.0000,$
 ENDSTYLE
 TABLE FILE HTFTE01
 BY DESCRIPCION_FUENTE AS ' '
 BY ID_FUENTE NOPRINT
 WHERE DESCRIPCION_FUENTE NE MISSING AND DESCRIPCION_FUENTE NE ' ' AND DESCRIPCION_FUENTE NE '  '
 ON TABLE SET STYLE *
 TYPE=REPORT,     UNITS=CM,     SQUEEZE=ON,     ORIENTATION=PORTRAIT, PAGECOLOR=RGB(255 255 255),$
 TYPE=REPORT,     GRID=ON,     FONT='Open Sans',     SIZE=9,     COLOR='BLACK',     BACKCOLOR=RGB(255 255 255),     STYLE=NORMAL,$
 TYPE=DATA,     FONT='Open Sans',     SIZE=9,     BACKCOLOR=( 'WHITE' RGB(222 222 222) ),$
